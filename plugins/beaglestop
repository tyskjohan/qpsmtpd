sub hook_data_post {
	my ($self, $transaction) = @_;

	my $subject = $transaction->header->get('Subject');

	if ($subject =~ /^455$/ || $subject =~ /^557$/ ||
		$subject =~ /^56757$/ ||
		$subject =~ /^586876$/ || $subject =~ /^1545453$/) {
		
			return (DENY, "W32.Beagle.FC");

	}
	return DECLINED;
}
